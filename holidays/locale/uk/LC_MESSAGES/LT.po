#. New Year's Day.
#: ./holidays/countries/lithuania.py:44
msgid "Naujųjų metų diena"
msgstr "Новий рік"

#. Day of Restoration of the State of Lithuania.
#: ./holidays/countries/lithuania.py:47
msgid "Lietuvos valstybės atkūrimo diena"
msgstr "День відновлення Литовської держави"

#. Day of Restoration of Independence of Lithuania.
#: ./holidays/countries/lithuania.py:50
msgid "Lietuvos nepriklausomybės atkūrimo diena"
msgstr "День відновлення незалежності Литви"

#. Easter.
#: ./holidays/countries/lithuania.py:53
msgid "Šv. Velykos"
msgstr "Великдень"

#. Easter Monday.
#: ./holidays/countries/lithuania.py:56
msgid "Antroji šv. Velykų diena"
msgstr "Великодній понеділок"

#. International Workers' Day.
#: ./holidays/countries/lithuania.py:59
msgid "Tarptautinė darbo diena"
msgstr "Міжнародний день трудящих"

#. Mother's Day.
#: ./holidays/countries/lithuania.py:62
msgid "Motinos diena"
msgstr "День матері"

#. Father's Day.
#: ./holidays/countries/lithuania.py:65
msgid "Tėvo diena"
msgstr "День батька"

#. Day of Dew and Saint John.
#: ./holidays/countries/lithuania.py:69
msgid "Rasos ir Joninių diena"
msgstr "День роси та День Івана Купала"

#. Statehood Day.
#: ./holidays/countries/lithuania.py:74
msgid ""
"Valstybės (Lietuvos karaliaus Mindaugo karūnavimo) ir Tautiškos giesmės diena"
msgstr "День державності та День національного гімну"

#. Assumption Day.
#: ./holidays/countries/lithuania.py:83
msgid "Žolinė (Švč. Mergelės Marijos ėmimo į dangų diena)"
msgstr "Внебовзяття Пресвятої Діви Марії"

#. All Saints' Day.
#: ./holidays/countries/lithuania.py:86
msgid "Visų Šventųjų diena"
msgstr "День усіх святих"

#. All Souls' Day.
#: ./holidays/countries/lithuania.py:90
msgid "Mirusiųjų atminimo (Vėlinių) diena"
msgstr "День пам'яті (День всіх померлих)"

#. Christmas Eve.
#: ./holidays/countries/lithuania.py:93
msgid "Kūčių diena"
msgstr "Святий вечір"

#. Christmas Day.
#: ./holidays/countries/lithuania.py:96
msgid "Šv. Kalėdų pirma diena"
msgstr "Різдво Христове"

#. Second Day of Christmas.
#: ./holidays/countries/lithuania.py:99
msgid "Šv. Kalėdų antra diena"
msgstr "Другий день Різдва"
