#. 100th anniversary of the adoption of the Declaration of the Slovak Nation.
#: holidays/countries/slovakia.py:35
msgid "100. výročie prijatia Deklarácie slovenského národa"
msgstr "100-а річниця прийняття Декларації словацької нації"

#. Day of the Establishment of the Slovak Republic.
#: holidays/countries/slovakia.py:56
msgid "Deň vzniku Slovenskej republiky"
msgstr "День утворення Словацької Республіки"

#. Epiphany.
#: holidays/countries/slovakia.py:59
msgid "Zjavenie Pána (Traja králi a vianočnýsviatok pravoslávnych kresťanov)"
msgstr "Богоявлення (Три царі і православне Різдво Христове)"

#. Good Friday.
#: holidays/countries/slovakia.py:65
msgid "Veľký piatok"
msgstr "Страсна пʼятниця"

#. Easter Monday.
#: holidays/countries/slovakia.py:68
msgid "Veľkonočný pondelok"
msgstr "Великодній понеділок"

#. Labor Day.
#: holidays/countries/slovakia.py:71
msgid "Sviatok práce"
msgstr "День праці"

#. Day of Victory over Fascism.
#: holidays/countries/slovakia.py:76
msgid "Deň víťazstva nad fašizmom"
msgstr "День перемоги над фашизмом"

#. St. Cyril and Methodius Day.
#: holidays/countries/slovakia.py:80
msgid "Sviatok svätého Cyrila a svätého Metoda"
msgstr "День Святих Кирила та Мефодія"

#. Slovak National Uprising Anniversary.
#: holidays/countries/slovakia.py:87
msgid "Výročie Slovenského národného povstania"
msgstr "Річниця Словацького національного повстання"

#. Constitution Day.
#: holidays/countries/slovakia.py:93
msgid "Deň Ústavy Slovenskej republiky"
msgstr "День конституції Словацької Республіки"

#. Day of Our Lady of the Seven Sorrows.
#: holidays/countries/slovakia.py:96
msgid "Sedembolestná Panna Mária"
msgstr "День Божої Матері семи скорбот"

#. Day of the Establishment of the Independent Czech-Slovak State.
#: holidays/countries/slovakia.py:102
msgid "Deň vzniku samostatného česko-slovenského štátu"
msgstr "День створення незалежної чесько-словацької держави"

#. All Saints' Day.
#: holidays/countries/slovakia.py:108
msgid "Sviatok Všetkých svätých"
msgstr "День усіх святих"

#. Struggle for Freedom and Democracy Day.
#: holidays/countries/slovakia.py:112
msgid "Deň boja za slobodu a demokraciu"
msgstr "День боротьби за свободу та демократію"

#. Christmas Eve.
#: holidays/countries/slovakia.py:115
msgid "Štedrý deň"
msgstr "Святий вечір"

#. Christmas Day.
#: holidays/countries/slovakia.py:118
msgid "Prvý sviatok vianočný"
msgstr "Різдво Христове"

#. Second Day of Christmas.
#: holidays/countries/slovakia.py:121
msgid "Druhý sviatok vianočný"
msgstr "Другий день Різдва"
