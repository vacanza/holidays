#. Sultan Hassanal Bolkiah's Golden Jubilee celebration
#: ./holidays/countries/brunei.py:65
msgid "Jubli Emas Sultan Hassanal Bolkiah"
msgstr "พระราชพิธีกาญจนาภิเษกสมเด็จพระราชาธิบดีสุลต่านฮัสซานัล โบลเกียห์"

#: ./holidays/countries/brunei.py:60
#, c-format
msgid "%s* (*anggaran)"
msgstr "%s* (*โดยประมาณ)"

#: ./holidays/countries/brunei.py:88 ./holidays/countries/brunei.py:190
#, c-format
msgid "%s - Diperhatikan"
msgstr "ชดเชย%s"

#. New Year's Day
#: ./holidays/countries/brunei.py:96
msgid "Awal Tahun Masihi"
msgstr "วันขึ้นปีใหม่"

#. Lunar New Year
#: ./holidays/countries/brunei.py:102
msgid "Tahun Baru Cina"
msgstr "วันตรุษจีน"

#. National Day
#: ./holidays/countries/brunei.py:109
msgid "Hari Kebangsaan"
msgstr "วันชาติบรูไน"

#. Armed Forces Day
#: ./holidays/countries/brunei.py:118
msgid "Hari Angkatan Bersenjata Diraja Brunei"
msgstr "วันกองทัพบรูไน"

#. Sultan Hassanal Bolkiah's Birthday
#: ./holidays/countries/brunei.py:127
msgid "Hari Keputeraan KDYMM Sultan Brunei"
msgstr "วันเฉลิมพระชนมพรรษาสมเด็จพระราชาธิบดีสุลต่านฮัสซานัล โบลเกียห์"

#. Christmas Day
#: ./holidays/countries/brunei.py:134
msgid "Hari Natal"
msgstr "วันคริสต์มาส"

#. Isra Mi'raj
#: ./holidays/countries/brunei.py:143
msgid "Israk dan Mikraj"
msgstr "วันเมี๊ยะราจ"

#. First Day of Ramadan
#: ./holidays/countries/brunei.py:150
msgid "Hari Pertama Berpuasa"
msgstr "วันแรกการถือศีลอด"

#. Anniversary of the revelation of the Quran
#: ./holidays/countries/brunei.py:157
msgid "Hari Nuzul Al-Quran"
msgstr "วันนูซุลอัลกุรอาน"

#. Eid al-Fitr
#: ./holidays/countries/brunei.py:172
msgid "Hari Raya Aidil Fitri"
msgstr "วันอีฎิ้ลฟิตริ"

#. Eid al-Adha
#: ./holidays/countries/brunei.py:203
msgid "Hari Raya Aidil Adha"
msgstr "วันอีดิ้ลอัฎฮา"

#. Islamic New Year
#: ./holidays/countries/brunei.py:210
msgid "Awal Tahun Hijrah"
msgstr "วันขึ้นปีใหม่อิสลาม"

#. Birth of the Prophet
#: ./holidays/countries/brunei.py:217
msgid "Maulidur Rasul"
msgstr "วันเมาลิดนบี"
