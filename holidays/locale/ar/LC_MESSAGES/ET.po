#. %s (estimated).
#, c-format
msgid "%s (ግምት)"
msgstr "(تقدير) %s"

#. Christmas Day.
msgid "ገና"
msgstr "عيد الميلاد"

#. Epiphany.
msgid "ጥምቀት"
msgstr "عيد الغطاس"

#. Adwa Victory Day.
msgid "አድዋ"
msgstr "العدوة"

#. Good Friday.
msgid "ስቅለት"
msgstr "جمعة جيدة"

#. Easter Sunday.
msgid "ፋሲካ"
msgstr "عيد الفصح"

#. Workers' Day.
msgid "የሰራተኞች ቀን"
msgstr "يوم العمال"

#. Patriots' Day.
msgid "የአርበኞች ቀን"
msgstr "يوم الوطنيين"

#. Downfall of Dergue Regime Day.
msgid "ደርግ የወደቀበት ቀን"
msgstr "يوم سقوط ديرج"

#. Ethiopian New Year.
msgid "እንቁጣጣሽ"
msgstr "السنة الإثيوبية الجديدة"

#. Finding of True Cross.
msgid "መስቀል"
msgstr "مسكل"

#. Revolution Day.
msgid "የአብዮት ቀን"
msgstr "يوم الثورة"

#. October Revolution Day.
msgid "የጥቅምት አብዮት ቀን"
msgstr "يوم ثورة أكتوبر"

#. Eid al-Fitr.
msgid "ኢድ አልፈጥር"
msgstr "عيد الفطر"

#. Eid al-Adha.
msgid "አረፋ"
msgstr "عيد الأضحى"

#. Prophet's Birthday.
msgid "መውሊድ"
msgstr "عيد المولد النبوي"
