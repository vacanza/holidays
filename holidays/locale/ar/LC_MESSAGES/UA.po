#. New Year's Day.
#: ./holidays/countries/ukraine.py:211
msgid "Новий рік"
msgstr "السنة الجديدة"

#. Christmas (Julian calendar).
#: ./holidays/countries/ukraine.py:216
msgid "Різдво Христове (за юліанським календарем)"
msgstr "عيد الميلاد (حسب التقويم اليولياني)"

#. International Women's Day.
#: ./holidays/countries/ukraine.py:221
msgid "Міжнародний жіночий день"
msgstr "اليوم العالمي للمرأة"

#. Easter Sunday (Pascha).
#: ./holidays/countries/ukraine.py:227
msgid "Великдень (Пасха)"
msgstr "عيد الفصح"

#. Holy Trinity Day.
#: ./holidays/countries/ukraine.py:230
msgid "Трійця"
msgstr "الثالوث"

#. Labour Day.
#: ./holidays/countries/ukraine.py:234
msgid "День праці"
msgstr "عيد العمال"

#. International Workers' Solidarity Day.
#: ./holidays/countries/ukraine.py:237
