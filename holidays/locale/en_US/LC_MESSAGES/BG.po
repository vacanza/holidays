#. %s (observed).
#, c-format
msgid "%s (почивен ден)"
msgstr "%s (observed)"

#. Holy Saturday.
msgid "Велика събота"
msgstr "Holy Saturday"

#. Easter.
msgid "Великден"
msgstr "Easter"

#. New Year's Day.
msgid "Нова година"
msgstr "New Year's Day"

#. Liberation Day.
msgid "Ден на Освобождението на България от османско иго"
msgstr "Liberation Day"

#. Good Friday.
msgid "Велики петък"
msgstr "Good Friday"

#. Labor Day and International Workers' Solidarity Day.
msgid "Ден на труда и на международната работническа солидарност"
msgstr "Labor Day and International Workers' Solidarity Day"

#. Saint George's Day, Day of the Bulgarian Army.
msgid "Гергьовден, Ден на храбростта и Българската армия"
msgstr "Saint George's Day, Day of the Bulgarian Army"

#. Day of the Holy Brothers Cyril and Methodius, Bulgarian Alphabet, Enlightenment, Culture and
#. Slavonic Literature.
msgid ""
"Ден на светите братя Кирил и Методий, на българската азбука, просвета и култура и на славянската "
"книжовност"
msgstr "Day of Slavonic Alphabet, Bulgarian Enlightenment and Culture"

#. Day of Bulgarian Enlightenment and Culture and Slavonic Alphabet.
msgid "Ден на българската просвета и култура и на славянската писменост"
msgstr "Day of Bulgarian Enlightenment and Culture and Slavonic Alphabet"

#. Unification Day.
msgid "Ден на Съединението"
msgstr "Unification Day"

#. Freedom Day.
msgid "Ден на свободата"
msgstr "Freedom Day"

#. Independence Day.
msgid "Ден на Независимостта на България"
msgstr "Independence Day"

#. Christmas Eve.
msgid "Бъдни вечер"
msgstr "Christmas Eve"

#. Christmas Day.
msgid "Рождество Христово"
msgstr "Christmas Day"

#. %s (from 2pm).
#, c-format
msgid "%s (след 14 ч.)"
msgstr "%s (from 2pm)"

#. The Day of the People's Awakeners.
msgid "Ден на народните будители"
msgstr "The Day of the People's Awakeners"

#. Substituted date format.
msgid "%d.%m.%Y"
msgstr "%m/%d/%Y"

#. Day off (substituted from %s).
#, c-format
msgid "Почивен ден (прехвърлен от %s)"
msgstr "Day off (substituted from %s)"

#. Official Holiday.
msgid "Официален празник"
msgstr "Official Holiday"

#. Non-working day.
msgid "Неприсъствен ден"
msgstr "Non-working day"
